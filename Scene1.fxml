<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="scene1Pane" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.Controller">
   <children>
      <Button fx:id="selectButton" alignment="CENTER" layoutX="103.0" layoutY="204.0" mnemonicParsing="false" onAction="#selectFile" prefHeight="47.0" prefWidth="136.0" text="Select File">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label fx:id="scene1Label" alignment="CENTER" layoutY="20.0" prefHeight="30.0" prefWidth="600.0" text="Please Select the Excel file:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="selectButton1" alignment="CENTER" layoutX="361.0" layoutY="204.0" mnemonicParsing="false" onAction="#switchScenes" prefHeight="47.0" prefWidth="136.0" text="Next">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
